/// getRoomName(rm_index)

var ri = argument[0];

switch ri {
	case rm_forest: {
		return "Everdusk Grove"; break;
	}
	case rm_dungeon: {
		return "Francis' Dungeon"; break;
	}
	case rm_dungeon2: {
		return "Francis' Dungeon 2"; break;
	}
	case rm_boss_funfairy: {
		return "Fairy's Great-Lodge"; break;
	}
	case rm_factory: {
		return "Lamplight Factory"; break;
	}
}