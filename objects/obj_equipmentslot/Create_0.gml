event_inherited();
slot = global.equipmentSlot;
name = "";
switch slot {
	case EquipmentSlots.LeftHand1: {
		name = "Main Hand 1";
		break;
	}
	case EquipmentSlots.RightHand1: {
		name = "Off Hand 1";
		break;
	}
	case EquipmentSlots.LeftHand2: {
		name = "Main Hand 2";
		break;
	}
	case EquipmentSlots.RightHand2: {
		name = "Off Hand 2";
		break;
	}
	case EquipmentSlots.LeftRing1: {
		name = "Left Ring 1";
		break;
	}
	case EquipmentSlots.LeftRing2: {
		name = "Left Ring 2";
		break;
	}
	case EquipmentSlots.RightRing1: {
		name = "Right Ring 1";
		break;
	}
	case EquipmentSlots.RightRing2: {
		name = "Right Ring 2";
		break;
	}
	case EquipmentSlots.Head: {
		name = "Head";
		break;
	}
}
up = global.up;
down = global.down;
left = global.left;
right = global.right;
